    "def max_prod_naive(arr = None):\n",
    "    product = 0\n",
    "    for i in range(arr):\n",
    "        for j in range(i+1,arr):\n",
    "            product = max(product,i * j)\n",
    "    return product\n",
    "\n",
    "print(max_prod_naive(arr=4))"
   ]
  }
